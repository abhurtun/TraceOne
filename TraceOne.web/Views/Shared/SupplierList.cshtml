<table >
    <thead>
        <tr>
            <th>Name</th>
            <th>Id</th>
            <th>Address</th>
        </tr>
    </thead>
     <tbody data-bind="foreach: suppliers" >
         <tr>
             <td data-bind="text: Name"></td>
             <td data-bind="text: Id"></td>
             <td data-bind="text: Address"></td>
         </tr>
     </tbody>
 </table>
 <br />
 <input type="button" id="btnGetSuppliers" value="Search" data-bind="click: getSuppliers" />